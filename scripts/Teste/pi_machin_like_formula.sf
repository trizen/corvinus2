    4 * (
             (183 * Math.atan2(1,     239)) +
             ( 32 * Math.atan2(1,    1023)) -
             ( 68 * Math.atan2(1,    5832)) +
             ( 12 * Math.atan2(1,  110443)) -
             ( 12 * Math.atan2(1, 4841182)) -
             (100 * Math.atan2(1, 6826318))
        )
   );


