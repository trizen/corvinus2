#!/usr/bin/ruby

#
## http://rosettacode.org/wiki/Binary_search
#

func binary_search(a, i) {
 
    var l = 0;
    var h = a.sfarsit;
 
    cat_timp (l <= h) {
        var mid = (h+l / 2 -> int);
        a[mid] > i && (h = mid-1; sari);
        a[mid] < i && (l = mid+1; sari);
        return mid;
    }
 
    return -1;
}

spune binary_search(1..100, 23);
